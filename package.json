  "description": "Angular 7 application with authentication (PEAN = PostgreSQL, Express, Angular, Node.js)",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "ng serve",
    "dev": "concurrently \"ng serve --host 0.0.0.0 --disable-host-check --proxy-config proxy.conf.json\" \"tsc -w -p server\" \"cross-env NODE_ENV=development nodemon dist/server/app.js\"",
    "prod": "concurrently \"ng build --prod --aot=true && tsc -p server && cross-env NODE_ENV=production node dist/server/app.js\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
